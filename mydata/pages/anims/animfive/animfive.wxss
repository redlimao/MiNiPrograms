/**animfive.wxss**/
.box{width: 200rpx;height: 200rpx;margin: 100rpx auto;background-color: #f4f4f4;position: relative;}
    .circle{position: absolute;width:40rpx;height: 40rpx;background-color: blueviolet;top: 0;left:0;right: 0;bottom: 0;margin: auto;border-radius: 50%;transform-origin:20rpx 100rpx;}
    .circle:nth-child(1){transform: translate(0,-80rpx) rotate(0deg);opacity: 1;animation: opacity1 .8s steps(1,start) infinite;}
    .circle:nth-child(2){transform: translate(0,-80rpx) rotate(45deg);opacity: .875;animation: opacity2 .8s steps(1,start) infinite;}
    .circle:nth-child(3){transform: translate(0,-80rpx) rotate(90deg);opacity: .75;animation: opacity3 .8s steps(1,start) infinite;}
    .circle:nth-child(4){transform: translate(0,-80rpx) rotate(135deg);opacity: .625;animation: opacity4 .8s steps(1,start) infinite;}
    .circle:nth-child(5){transform: translate(0,-80rpx) rotate(180deg);opacity: .5;animation: opacity5 .8s steps(1,start) infinite;}
    .circle:nth-child(6){transform: translate(0,-80rpx) rotate(225deg);opacity: .375;animation: opacity6 .8s steps(1,start) infinite;}
    .circle:nth-child(7){transform: translate(0,-80rpx) rotate(270deg);opacity: .25;animation: opacity7 .8s steps(1,start) infinite;}
    .circle:nth-child(8){transform: translate(0,-80rpx) rotate(315deg);opacity: .125;animation: opacity8 .8s steps(1,start) infinite;}
    @keyframes opacity1{
        0%{opacity: 1;}
        12.5%{opacity:1;}
        25%{opacity: .875;}
        37.5%{opacity: .75;}
        50%{opacity: .625;}
        62.5%{opacity: .50;}
        75%{opacity: .375;}
        87.5%{opacity: .25}
        100%{opacity: .125;}
    } 
    @keyframes opacity2{
        0%{opacity: 1;}
        12.5%{opacity:.875;}
        25%{opacity: .75;}
        37.5%{opacity: .625;}
        50%{opacity: .5;}
        62.5%{opacity: .375;}
        75%{opacity: .25;}
        87.5%{opacity: .125}
        100%{opacity: 1;}
    } 
